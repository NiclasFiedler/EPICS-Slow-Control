record(ai, "SAA$(boardid):VERSION")
{
	field(DESC, "Get version of the SerialAdapter ASIC.")
	field(DTYP, "DevAiSAA")
	field(PINI, "RUN")
	field(INP, "@$(n_saa).$(boardid)")
}

record(ai, "SAA$(boardid):LM75:TEMP")
{
	field(DESC, "Read temperature from LM75.")
	field(DTYP, "DevLm75")
	#field(PREC, "1")
	#field(ESLO, "1e-1")
	field(EGU,  "°C") 
	field(SCAN, "5 second")
	#field(LINR, "LINEAR")
	#field(HOPR, "75") # Set high operating range
	#field(LOPR, "-25") # Set low operating range
	#field(HIHI, "") Set upper alarm limit
	#field(LOLO, "") Set lower alarm limit
	#field(HIGH, "") Set upper warning limit
	#field(LOW, "")  Set lower warning limit
	#field(HYST, "") Set deadband around alarm limits to avoid to many alarms
	#field(HHSV,"MINOR") Set HIHI alarm severity
	#field(LLSV,"MINOR") Set LOLO alarm severity
	#field(HSV,"MAJOR") Set HIGH alarm severity
	#field(LSV,"MAJOR") Set LOW alarm severity
	field(INP, "@$(bitrate).$(boardid)")
}

