record(ai, "SAA$(boardid):DAC:GET:CH$(adschannel)")
{
        field(DESC, "Read current DAC Value of channel.")
        field(DTYP, "DevAiDAC")
	field(PINI, "RUN")
        field(INP, "@$(adschannel).$(boardid)")
} 

record(ao, "SAA$(boardid):DAC:SET:CH$(adschannel)")
{
        field(DESC, "Set DAC Value of channel.")
        field(DTYP, "DevAoDAC")	
	field(OUT, "@$(adschannel).$(boardid)")
	field(FLNK, "SAA$(boardid):DAC:GET:CH$(adschannel)")
}

